    "\n",
    "palindromeNum = n \n",
    "revNum = 0\n",
    "\n",
    "while n != 0:\n",
    "    digit = n % 10\n",
    "    revNum = (revNum * 10 ) + digit\n",
    "    n = n // 10\n",
    "\n",
    "print(revNum)\n",
    "\n",
    "if revNum == palindromeNum:\n",
    "    print(True)\n",
    "else:\n",
    "    print(False)"
