    "arr1 = [2,6,1,2]\n",
    "arr2 = [1,2,3,4,2]\n",
    "\n",
    "def intersection(arr1, arr2):\n",
    "\n",
    "    list = []\n",
    "\n",
    "    for i in arr1:\n",
    "        if i in arr2:\n",
    "            list.append(i)\n",
    "            arr2.remove(i)\n",
    "    \n",
    "    return list\n",
    "\n",
    "intersection(arr1, arr2)\n"
   ]
