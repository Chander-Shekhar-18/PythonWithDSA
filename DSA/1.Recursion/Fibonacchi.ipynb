    "def fibo(firstNum, secondNum, num):\n",
    "    if num <= 0:\n",
    "        return\n",
    "\n",
    "    print(firstNum)\n",
    "    print(secondNum)\n",
    "\n",
    "    thirdNum = firstNum + secondNum\n",
    "    print(thirdNum)\n",
    "\n",
    "    fibo(secondNum, thirdNum, num - 1)  # Decrement num by 1\n"
   ]
