   "source": [
    "def find_last(arr, num, index=-1):\n",
    "    if index == -len(arr) - 1:\n",
    "        return -1\n",
    "    \n",
    "    if arr[index] == num:\n",
    "        return len(arr) + index\n",
    "    \n",
    "    return find_last(arr, num, index - 1)\n",
    "\n",
    "\n",
    "arr = [1, 2, 3, 2, 4, 2, 5]\n",
    "num = 2\n",
    "print(find_last(arr, num))  # Output: 5\n"
   ]
  }
