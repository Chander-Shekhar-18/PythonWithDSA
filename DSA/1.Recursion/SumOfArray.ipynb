   "source": [
    "def sumOfArray(nums, index):\n",
    "    if index == len(nums) - 1:\n",
    "        return nums[index]\n",
    "\n",
    "    ans = nums[index] + sumOfArray(nums, index + 1)\n",
    "\n",
    "    return ans  \n"
   ]
