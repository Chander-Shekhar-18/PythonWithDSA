   "source": [
    "n = 123456\n",
    "\n",
    "oddSum = 0\n",
    "evenSum = 0\n",
    "\n",
    "while n != 0:\n",
    "    digit = n % 10\n",
    "    if digit % 2 == 0:\n",
    "        evenSum = evenSum + digit\n",
    "    else:\n",
    "        oddSum = oddSum + digit\n",
    "    \n",
    "    n = n // 10\n",
    "\n",
    "print(f'Sum of even digits are {evenSum}')\n",
    "print(f'Sum of odd digits are {oddSum}')"
   ]
  }
 ],
