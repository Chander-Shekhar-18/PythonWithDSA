    "def replaceChar(str, char1, char2):\n",
    "    newStr = ''\n",
    "    for i in str:\n",
    "        if i == char1:\n",
    "            newStr += char2\n",
    "        else:\n",
    "            newStr += i\n",
    "    \n",
    "    return newStr"
   ]
